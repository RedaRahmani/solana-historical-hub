{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
      {
        "src": "^/ui$",
        "dest": "/index.html"
      },
      {
        "src": "^/$",
        "dest": "/index.html"
      },
      {
        "src": "^/(.*\\.(js|css|png|jpg|jpeg|svg|ico|map))$",
        "dest": "/$1"
      },
      {
        "src": "^/(.*)$",
        "dest": "/api/index.js"
      }
    ],
  
  "env": {
    "NODE_ENV": "production"
  }
}